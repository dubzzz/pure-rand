{
  "name": "pure-rand",
  "version": "7.0.1",
  "description": " Pure random number generator written in TypeScript",
  "type": "commonjs",
  "main": "lib/pure-rand.js",
  "exports": {
    "./package.json": "./package.json",
    "./distribution/*": {
      "require": {
        "types": "./lib/distribution/*.d.ts",
        "default": "./lib/distribution/*.js"
      },
      "import": {
        "types": "./lib/esm/distribution/*.d.ts",
        "default": "./lib/esm/distribution/*.js"
      }
    },
    "./generator/*": {
      "require": {
        "types": "./lib/generator/*.d.ts",
        "default": "./lib/generator/*.js"
      },
      "import": {
        "types": "./lib/esm/generator/*.d.ts",
        "default": "./lib/esm/generator/*.js"
      }
    },
    "./types/*": {
      "require": {
        "types": "./lib/types/*.d.ts",
        "default": "./lib/types/*.js"
      },
      "import": {
        "types": "./lib/esm/types/*.d.ts",
        "default": "./lib/esm/types/*.js"
      }
    },
    ".": {
      "require": {
        "types": "./lib/types/pure-rand.d.ts",
        "default": "./lib/pure-rand.js"
      },
      "import": {
        "types": "./lib/esm/pure-rand.d.ts",
        "default": "./lib/esm/pure-rand.js"
      }
    }
  },
  "module": "lib/esm/pure-rand.js",
  "types": "lib/pure-rand.d.ts",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "packageManager": "pnpm@10.30.2",
  "scripts": {
    "format:check": "oxfmt --list-different",
    "format": "oxfmt",
    "typecheck": "tsc --noEmit",
    "build": "rolldown -c && cp lib/esm/*.d.ts lib/ && cp lib/esm/distribution/*.d.ts lib/distribution/ && cp lib/esm/generator/*.d.ts lib/generator/ && cp lib/esm/types/*.d.ts lib/types/ && cp package.esm-template.json lib/esm/package.json",
    "build:esm": "tsc --module es2015 --outDir lib/esm --moduleResolution node && tsc -p ./tsconfig.declaration.json --outDir lib/esm/types && cp package.esm-template.json lib/esm/package.json",
    "build:prod": "pnpm build && pnpm build:esm && node postbuild/main.mjs",
    "build:prod-ci": "cross-env EXPECT_GITHUB_SHA=true pnpm build:prod",
    "test": "vitest",
    "build:bench:old": "tsc --outDir lib-reference/",
    "build:bench:new": "tsc --outDir lib-test/",
    "bench": "node perf/benchmark.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dubzzz/pure-rand.git"
  },
  "author": "Nicolas DUBIEN <github@dubien.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dubzzz/pure-rand/issues"
  },
  "homepage": "https://github.com/dubzzz/pure-rand#readme",
  "devDependencies": {
    "@rollup/plugin-replace": "^6.0.3",
    "@types/node": "^24.10.13",
    "cross-env": "^10.1.0",
    "fast-check": "^4.5.3",
    "oxfmt": "^0.35.0",
    "rolldown": "1.0.0-rc.5",
    "rolldown-plugin-dts": "^0.22.1",
    "source-map-support": "^0.5.21",
    "tinybench": "^6.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "seed",
    "random",
    "prng",
    "generator",
    "pure",
    "rand",
    "mersenne",
    "random number generator",
    "fastest",
    "fast"
  ],
  "funding": [
    {
      "type": "individual",
      "url": "https://github.com/sponsors/dubzzz"
    },
    {
      "type": "opencollective",
      "url": "https://opencollective.com/fast-check"
    }
  ]
}
